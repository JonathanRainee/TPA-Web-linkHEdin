type Experience {
    ID: ID!
	UserID: String!
	Title: String!
	EmploymentType: String! 
	CompanyName: String! 
	Location: String! 
	Active: Boolean!
	StartYear: String! 
	EndYear: String! 
	Industry: String! 
	Description: String! 
}

input newExperience {
    UserID: String!
	Title: String!
	EmploymentType: String! 
	CompanyName: String! 
	Location: String! 
	Active: Boolean!
	StartYear: String! 
	EndYear: String! 
	Industry: String! 
	Description: String! 
}

extend type Query {
    userExperience(UserID: ID!): [Experience!]!
}

extend type Mutation {
    createExperience(input: newExperience!): String!
    updateExperience(id: ID!, input: newExperience!): String!
    deleteExperience(id: ID!): String!
}